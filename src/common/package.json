{
  "name": "@1shotapi/1shotpay-common",
  "version": "0.1.8",
  "description": "Shared types and utilities for 1ShotPay client and server SDKs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "1shotapi",
    "1shotpay",
    "1shotpay-common",
    "sdk"
  ],
  "author": "1Shot API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1shotapi/1shotpay-sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "inversify": "^7.11.0",
    "neverthrow": "^8.2.0",
    "reflect-metadata": "^0.2.2",
    "ts-brand": "^0.2.0",
    "zod": "^4.3.6"
  }
}
