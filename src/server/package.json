{
  "name": "@1shotapi/1shotpay-server-sdk",
  "version": "0.0.1",
  "description": "Server SDK for 1ShotPay API integration",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "1shotapi",
    "1shotpay",
    "1shotpay-server-sdk",
    "sdk",
    "server"
  ],
  "author": "1Shot API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1shotapi/1shotpay-sdk.git"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@1shotapi/1shotpay-common": "workspace:*",
    "neverthrow-result-utils": "^3.0.0"
  }
}
